{
  "name": "alexa-service",
  "version": "8.3.5",
  "description": "Alexa Service",
  "main": "command.js",
  "scripts": {
    "test": "mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "start": "node command.js",
    "coverage": "nyc npm test",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/octoblu/alexa-service.git"
  },
  "keywords": [
    "Alexa",
    "Octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/alexa-service/issues"
  },
  "homepage": "https://github.com/octoblu/alexa-service",
  "dependencies": {
    "@octoblu/redis-ns": "^1.3.1",
    "alexa-app": "^4.0.1",
    "async": "^2.0.0-rc.6",
    "body-parser": "^1.15.2",
    "coffee-script": "^1.9.3",
    "cors": "^2.7.1",
    "debug": "^2.2.0",
    "errorhandler": "^1.4.2",
    "express": "^4.14.0",
    "express-meshblu-healthcheck": "^1.0.1",
    "express-package-version": "0.0.2",
    "express-redis-pooled-client": "^2.0.0",
    "express-send-error": "^1.1.0",
    "ioredis": "^3.1.1",
    "lodash": "^4.13.1",
    "meshblu-config": "^7.0.0",
    "meshblu-http": "^9.9.0",
    "moment": "^2.13.0",
    "morgan": "^1.7.0",
    "normalize-url": "^1.5.3",
    "on-finished": "^2.3.0",
    "request": "^2.72.0",
    "server-destroy": "^1.0.1",
    "validator": "^7.0.0",
    "x509": "^0.3.0"
  },
  "devDependencies": {
    "chai": "^4.0.1",
    "env-test": "^1.0.0",
    "mocha": "^3.4.2",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^11.0.1",
    "shmock": "^0.8.1",
    "sinon": "^2.3.2",
    "sinon-chai": "^2.8.0",
    "uuid": "^3.0.1"
  },
  "nyc": {
    "cache": true,
